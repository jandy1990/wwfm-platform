{
  "name": "wwfm-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "debug": "node tools/debug-page.js",
    "debug:goal": "node tools/debug-page.js http://localhost:3001/goal/",
    "debug:mobile": "node tools/debug-page.js --mobile",
    "test:forms": "playwright test tests/e2e/forms",
    "test:forms:chromium": "playwright test tests/e2e/forms --project=chromium",
    "test:forms:mobile": "playwright test tests/e2e/forms --project='Mobile Chrome'",
    "test:quick": "playwright test tests/e2e/smoke-test.spec.ts --project=chromium",
    "test:smoke": "playwright test tests/e2e/smoke-test.spec.ts --project=chromium",
    "test:critical": "playwright test tests/e2e/forms/app-form-complete.spec.ts tests/e2e/forms/dosage-form-complete.spec.ts tests/e2e/forms/hobby-form-complete.spec.ts tests/e2e/forms/practice-form-complete.spec.ts tests/e2e/forms/community-form-complete.spec.ts tests/e2e/forms/lifestyle-form-complete.spec.ts tests/e2e/forms/purchase-form-complete.spec.ts tests/e2e/forms/financial-form-complete.spec.ts tests/e2e/forms/session-form-complete.spec.ts --project=chromium",
    "test:critical-no-session": "playwright test tests/e2e/forms/app-form-complete.spec.ts tests/e2e/forms/dosage-form-complete.spec.ts tests/e2e/forms/practice-form-complete.spec.ts tests/e2e/forms/community-form-complete.spec.ts tests/e2e/forms/lifestyle-form-complete.spec.ts tests/e2e/forms/purchase-form-complete.spec.ts tests/e2e/forms/financial-form-complete.spec.ts --project=chromium",
    "test:critical-full": "playwright test tests/e2e/forms/*-complete.spec.ts --project=chromium",
    "test:forms:ui": "playwright test tests/e2e/forms --ui",
    "test:forms:debug": "playwright test tests/e2e/forms --debug",
    "test:forms:headed": "playwright test tests/e2e/forms --headed",
    "test:forms:report": "playwright show-report",
    "test:forms:ci": "playwright test tests/e2e/forms --reporter=html,github",
    "test:forms:all": "node scripts/test-all-forms.js",
    "test:forms:validate": "node -r dotenv/config scripts/validate-database.ts",
    "test:setup": "node tests/setup/complete-test-setup.js",
    "test:reset": "node tests/setup/reset-test-data.js",
    "test:connection": "node scripts/test-connection.js",
    "test:better": "node tests/utils/better-test-runner.js",
    "test:analyze": "node tests/utils/analyze-test-output.js",
    "test:capture": "npm run test:forms 2>&1 | tee test-output.txt && npm run test:analyze test-output.txt",
    "generate:ai-solutions": "tsx scripts/ai-solution-generator/index.ts",
    "generate:smart": "tsx scripts/ai-solution-generator/index.ts --smart-select",
    "generate:validate": "tsx scripts/ai-solution-generator/validate-and-fix.ts",
    "generate:check": "tsx scripts/ai-solution-generator/check-progress.ts",
    "test:ai-generator": "tsx scripts/ai-solution-generator/test.ts",
    "test:ai-dropdown": "tsx scripts/ai-solution-generator/test-dropdown-validation.ts",
    "test:ai-mapper": "tsx scripts/ai-solution-generator/test-value-mapper.ts",
    "quality:check": "tsx scripts/ai-solution-generator/quality-pipeline.ts check",
    "quality:orchestrate": "tsx scripts/ai-solution-generator/quality-pipeline.ts orchestrate",
    "quality:dashboard": "tsx scripts/ai-solution-generator/quality-pipeline.ts dashboard",
    "quality:test": "tsx scripts/ai-solution-generator/quality-pipeline.ts test",
    "quality:test-100": "tsx scripts/ai-solution-generator/quality-test-100.ts",
    "quality:migrate": "tsx scripts/ai-solution-generator/quality-pipeline.ts migrate",
    "expand:solution": "tsx scripts/ai-solution-generator/solution-goal-expander.ts",
    "expand:dry-run": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --dry-run",
    "expand:category": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category",
    "expand:strict": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --strict-mode",
    "expand:test": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --dry-run --limit 2 --max-goals 2",
    "expand:books": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category books_courses --strict-mode false --limit 100",
    "expand:apps": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category apps_software --strict-mode false --limit 100",
    "expand:habits": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category habits_routines --strict-mode false --limit 100",
    "expand:products": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category products_devices --strict-mode false --limit 100",
    "expand:exercise": "tsx scripts/ai-solution-generator/solution-goal-expander.ts --category exercise_movement --strict-mode false --limit 80",
    "expand:quality": "tsx scripts/ai-solution-generator/solution-goal-expander-quality.ts",
    "monitor:expansion": "tsx scripts/ai-solution-generator/monitor-expansion.ts",
    "expand:quality:sleep": "tsx scripts/ai-solution-generator/solution-goal-expander-quality.ts --category sleep --mode auto --batch-size 10",
    "expand:quality:books": "tsx scripts/ai-solution-generator/solution-goal-expander-quality.ts --category books_courses --mode zero --batch-size 20",
    "expand:quality:apps": "tsx scripts/ai-solution-generator/solution-goal-expander-quality.ts --category apps_software --mode zero --batch-size 15"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "chalk": "^5.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.516.0",
    "next": "15.3.2",
    "ora": "^8.2.0",
    "puppeteer": "^24.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-swipeable": "^7.0.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "playwright": "^1.54.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
